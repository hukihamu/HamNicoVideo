{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "sourceMap": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"]
    },
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    
  },
  "exclude": [
    "node_modules"
  ]
}